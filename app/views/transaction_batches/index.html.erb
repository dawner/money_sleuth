<p id="notice"><%= notice %></p>
<%= link_to 'Back', root_path %>

<h1>Transaction Batches</h1>

<table>
  <thead>
    <tr>
      <th>Upload ref</th>
      <th>Bank</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transaction_batches.each do |transaction_batch| %>
      <tr>
        <td><%= transaction_batch.bank.name %></td>
        <td><%= link_to 'Show', transaction_batch %></td>
        <td><%= link_to 'Edit', edit_transaction_batch_path(transaction_batch) %></td>
        <td><%= link_to 'Destroy', transaction_batch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction Batch', new_transaction_batch_path %>
